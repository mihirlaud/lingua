import stdio

define int a
define int b
assign 1 to a
assign 1 to b

for every int x from 0 to 10
	call stdio::print(input: a)
	assign 10 to x
	assign a + b to a
	assign a - b to b
	endfor

define void fibonacci(int range)
	define int i
	define int j
	assign 1 to i
	assign 1 to j
	
	for every int x from 0 to range
		assign i + j to i
		assign i - j to j
		endfor
	
	call stdio::print(input: i)
	enddef

call fibonacci(range: 10)